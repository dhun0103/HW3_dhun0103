   "source": [
    "def factorial(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial(n - 1)\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    for i in range(15):\n",
    "        print(f\"Factorial of {i} : {factorial(i)}\")"
   ]
  }
